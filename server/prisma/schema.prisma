generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Employee {
  id         BigInt  @id @default(autoincrement())
  name       String? @default("\"\"")
  last_name  String? @default("\"\"")
  position   String?
  department BigInt?
  salary     BigInt?
}
